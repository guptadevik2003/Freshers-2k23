<body>

    <!-- Navbar Section -->
    <div class="navbar_section">
        <nav class=" container navbar navbar-expand-lg navbar-dark">
            <a class="navbar-brand text-white" href="/">
                Freshers 2<span>K</span>23
            </a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <i onclick="showMenu()" id="showMenu" class="text-white fa fa-bars"></i>
                <i onclick="hideMenu()" id="hideMenu" class="text-white fa fa-times"></i>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item">
                        <a class="nav-link" href="/">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/#about">About</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/committee" target="_self" id="active-tab">Committee</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/#contact">Contact</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/admin/login">Admin</a>
                    </li>
                </ul>
            </div>
        </nav>
    </div>
    
    <!-- Hero Section -->
    <div class="hero_section container">

        <% data.forEach(committee => { %>
            <div class="committee_container" id="<%= committee.name.toLowerCase() %>">
                
                <h1><%= committee.name.toUpperCase() %> COMMITTEE</h1>
                <h4><span>Coordinator - </span><%= committee.faculty.name %></h4>
                
                <% let count = 0 %>
                <% let members = committee.members %>

                <% while(count < members.length){ %>
                    <div class="horizontal_container">

                        <% if(members[count]){ %>
                            <div class="member_container">

                                <img class="memberImage" loading="lazy" src="<%= members[count].image %>">
                                <h3 class="memberName"><%= members[count].name %></h3>

                            </div>
                        <% } %>

                        <% count++ %>

                        <% if(members[count]){ %>
                            <div class="member_container">

                                <img class="memberImage" loading="lazy" src="<%= members[count].image %>">
                                <h3 class="memberName"><%= members[count].name %></h3>

                            </div>
                        <% } %>

                        <% count++ %>

                    </div>
                <% } %>

            </div>
        <% }) %>
        
